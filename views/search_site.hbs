                                </a>
                            </li>
                            {{/if}}
                        {{/if}}
                        {{/with}}
                    </ul>
                </div>
            </div>
        </div>
        <div class="menu-sort">
            {{#with this.cateId}}
            {{#if categoryById}}
            <div class="menusub-choices">
                <span class="style-submenu menu0">
                    <i class="fas fa-sort-amount-up"></i> Sắp xếp
                </span>
                <span class="blur-style diff-char"> | </span>
                <a href="/search/{{Id}}/mode/most-study">
                    <span class="style-submenu menu1 blur-style" {{#if active_m_st}}id="active_most"{{/if}}>Học nhiều nhất</span>
                </a>
                <a href="/search/{{Id}}/mode/most-overall">
                    <span class="style-submenu menu2 blur-style"{{#if active_m_ov}}id="active_most"{{/if}}>Đánh giá cao nhất</span>
                </a>
                <a href="/search/{{Id}}/mode/newest">
                    <span class="style-submenu menu3 blur-style" {{#if active_new}}id="active_most"{{/if}}>Mới nhất</span>
                </a>
                <a href="/search/{{Id}}/mode/price-ascend">
                    <span class="style-submenu menu4 blur-style"{{#if active_m_ap}}id="active_most"{{/if}}>Giá thấp đến cao</span>
                </a>
                <a href="/search/{{Id}}/mode/price-descend">
                    <span class="style-submenu menu5 blur-style"{{#if active_m_dp}}id="active_most"{{/if}}>Giá cao đến thấp</span>
                </a>
            </div>
            {{else}}
            {{#if categoryByIdSub}}
