                                </a>
                            </li>
                            {{else}}
                            <li class="item {{#if active_5}}activate_choice noHover{{/if}}">
                                <a href="/search/keyword/{{Id}}/rating-5">
                                    <label class="text_star {{#if active_5}}active_text{{/if}}"> <span class="rating-choice"><i class="fas fa-star"></i></span> 5.0 </label>
                                </a>
                            </li>
                            <li class="item {{#if active_up_4}}activate_choice noHover{{/if}}">
                                <a href="/search/keyword/{{Id}}/rating-up-4">
                                    <label class="text_star {{#if active_up_4}}active_text{{/if}}"> <span class="rating-choice"><i class="fas fa-star"></i></span> 4.0 trở lên </label>
